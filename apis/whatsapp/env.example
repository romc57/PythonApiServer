# WhatsApp Personal API Configuration
# Copy this file to .env and fill in your actual values

# WhatsApp App Credentials
WHATSAPP_APP_ID=your_whatsapp_app_id
WHATSAPP_APP_SECRET=your_whatsapp_app_secret

# Session Configuration
WHATSAPP_SESSION_ID=your_session_id
WHATSAPP_WEBHOOK_URL=https://your-domain.com/whatsapp/webhook

# OAuth Configuration
WHATSAPP_REDIRECT_URI=http://localhost:8081/whatsapp/callback

# Optional: API Rate Limits
WHATSAPP_RATE_LIMIT=1000
WHATSAPP_RATE_WINDOW=3600

